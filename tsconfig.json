{
	"extends": "./.svelte-kit/tsconfig.json",
	"exclude": [
		"node_modules",
		"cdk.out"
	],
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"module": "es2022",
		"importsNotUsedAsValues": "remove"
		
	},
	"ts-node": {
        "esm": true,
        "experimentalSpecifierResolution": "node"
    }
}